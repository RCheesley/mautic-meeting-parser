{
  "name": "Drupal Meeting Parser",
  "version": "1.0",
  "description": "Parses Drupal meeting threads for public archiving",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.slack.com/*"],
      "js": ["js/scrape.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
